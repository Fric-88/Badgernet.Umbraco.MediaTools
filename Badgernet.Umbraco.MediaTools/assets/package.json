    "node": "^20.19.0"
  },
  "devDependencies": {
    "@hey-api/openapi-ts": "^0.46.3",
    "eslint": "^9.8.0",
    "lit": "^3.2.1",
    "typescript": "^5.8.2",
    "vite": "^6.2.2"
  }
}
